    {
      "request": "launch",
      "name": "debug testing",
      "type": "node",
      "program": "${workspaceFolder}/src/strategies/json.test.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "C:\\ProgramData\\chocolatey\\bin\\deno.EXE",
      "runtimeArgs": [
        "test",
        "--inspect-wait",
        "--allow-all"
      ],
      "attachSimplePort": 9229
    }
  ]
